{
  "id": "documenso",
  "name": "Documenso",
  "available": true,
  "exposable": true,
  "port": 3015,
  "dynamic_config": true,
  "tipi_version": 1,
  "version": "2.3.0",
  "categories": ["utilities", "productivity"],
  "short_desc": "The Open Source DocuSign Alternative",
  "description": "Documenso is an open source document signing platform. Sign documents digitally with a self-hosted solution that puts you in control of your data and trust infrastructure.",
  "author": "Documenso",
  "website": "https://documenso.com",
  "source": "https://github.com/documenso/documenso",
  "supported_architectures": ["amd64", "arm64"],

  "form_fields": [
    {
      "type": "random",
      "label": "NextAuth Secret",
      "env_variable": "DOCUMENSO_NEXTAUTH_SECRET",
      "required": true,
      "min": 64
    },
    {
      "type": "random",
      "label": "Postgres database password",
      "env_variable": "DOCUMENSO_DB_PASSWORD",
      "required": true,
      "min": 32
    },
    {
      "type": "text",
      "label": "SMTP From Name",
      "env_variable": "DOCUMENSO_SMTP_FROM_NAME",
      "required": true,
      "default": "Documenso"
    },
    {
      "type": "email",
      "label": "SMTP From Address",
      "env_variable": "DOCUMENSO_SMTP_FROM_ADDRESS",
      "required": true,
      "default": "noreply@example.com"
    },
    {
      "type": "text",
      "label": "SMTP Host",
      "env_variable": "DOCUMENSO_SMTP_HOST",
      "required": false,
      "hint": "Leave empty to use built-in mail server (Inbucket)"
    },
    {
      "type": "number",
      "label": "SMTP Port",
      "env_variable": "DOCUMENSO_SMTP_PORT",
      "required": false,
      "default": "587"
    },
    {
      "type": "text",
      "label": "SMTP Username",
      "env_variable": "DOCUMENSO_SMTP_USER",
      "required": false
    },
    {
      "type": "password",
      "label": "SMTP Password",
      "env_variable": "DOCUMENSO_SMTP_PASSWORD",
      "required": false
    }
  ]
}
